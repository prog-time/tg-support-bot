{
    "api_external_messages_update": {
        "type": "object",
        "required": [
            "source",
            "external_id"
        ],
        "properties": {
            "source": {
                "type": "string",
                "example": "live_chat"
            },
            "external_id": {
                "type": "string",
                "example": "u123"
            },
            "message_id": {
                "type": "integer",
                "example": "123456"
            },
            "text": {
                "type": "string",
                "example": "Привет"
            }
        }
    }
}
